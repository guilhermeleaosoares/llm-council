{
  "name": "llm-council",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "backend": "cd server && node index.js",
    "electron:dev": "concurrently \"NODE_ENV=development npm run dev\" \"wait-on http://localhost:5173 && unset ELECTRON_RUN_AS_NODE && NODE_ENV=development npx electron electron/main.cjs\"",
    "electron:build": "npm run build && unset ELECTRON_RUN_AS_NODE && electron-builder",
    "server": "concurrently \"npm run dev\" \"npm run backend\""
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@xyflow/react": "^12.10.1",
    "dagre": "^0.8.5",
    "firebase": "^11.1.0",
    "highlight.js": "^11.11.1",
    "katex": "^0.16.33",
    "lucide-react": "^0.460.0",
    "mermaid": "^11.12.3",
    "pdfjs-dist": "^4.9.155",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.28.0",
    "react-zoom-pan-pinch": "^3.7.0",
    "rehype-highlight": "^7.0.2",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.2.1",
    "electron": "^40.6.1",
    "electron-builder": "^26.8.1",
    "electron-is-dev": "^3.0.1",
    "vite": "^6.0.0",
    "wait-on": "^9.0.4"
  }
}
